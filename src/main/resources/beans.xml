<?xml version="1.0" encoding="UTF-8" ?>
<beans>
    <bean id="basebaseservice" class="com.minis.test.entity.BaseBaseService">
        <property type="com.minis.test.entity.AServiceImpl" name="as" ref="aservice"/>
    </bean>

    <bean id="aservice" class="com.minis.test.entity.AServiceImpl">
        <constructor-arg type="String" name="name" value="abc"/>
        <constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="com.minis.test.entity.BaseService" name="ref1" ref="baseservice"/>
    </bean>

    <bean id="baseservice" class="com.minis.test.entity.BaseService">
        <property type="com.minis.test.entity.BaseService" name="base" ref="basebaseservice"/>
    </bean>
</beans>